/*             
 * Linked Lists: Linear structure consisting of nodes. 
 * 				 Each Node contains a data part that links to the next node 
 */
public class LinkedList<T> {
	Node<T> head;
	Node<T> tail;
	int length;
	
	public LinkedList(Node<T> head, Node<T> tail, int length) {
		this.head = head;
		this.tail = tail;
		this.length = length;
	}
}
